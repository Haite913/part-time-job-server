<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team.ptjs.Biz.mapper.ApplianceListMapper">


    <select id="queryPage" resultType="com.team.ptjs.Api.entity.ApplianceList">
       select * from appliance_list where appliance_list.username = #{query.username}
    </select>
    <select id="getDetialById" resultType="com.team.ptjs.Api.vo.ApplianceListDetailVo">
        select * from appliance_list_detail where appliance_list_detail.job_id = #{id}
    </select>
</mapper>
